
  <div class="container">
      <div class="row">
          <div class="col-sm-6 col-md-4">
              <img src="<%= user.profile.picture %>" alt="avatar" class="img-circle img-responsive img-center">
              <p>Name: <%= user.profile.name %></p>
              <p>Email: <%= user.email %></p>
              <p>Address: <%= user.profile.address %></p>

              <a class="btn btn-primary" href="/edit-profile" role="button">Edit Profile</a>
          </div>
      </div>
  </div>

